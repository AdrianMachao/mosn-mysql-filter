{
    "servers": [
        {
            "default_log_path": "stdout",
            "default_log_level": "DEBUG",
            "processor": "auto",
            "listeners": [
                {
                    "name": "mysqlListener",
                    "address": "0.0.0.0:50051",
                    "bind_port": true,
                    "log_path": "stdout",
                    "filter_chains": [
                        {
                            "tls_context": {},
                            "filters": [
                                {
                                    "type": "mysql-proxy",
                                    "config": {
                                        "cluster": "mysqlCluster",
                                        "routes": [
                                          {
                                            "cluster": "mysqlCluster"
                                          }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "cluster_manager": {
        "clusters": [
            {
                "Name": "mysqlCluster",
                "type": "SIMPLE",
                "lb_type": "LB_RANDOM",
                "max_request_per_conn": 1024,
                "conn_buffer_limit_bytes": 32768,
                "hosts": [{ "address": "127.0.0.1:3306" }]
            }
        ]
    },
    "metrics": {
        "shm_zone": "metrics",
        "shm_size": "10MB",
        "flush_interval": "500ms",
        "sinks": [
            {
                "type": "prometheus",
                "config": {
                    "port": 34903
                }
            }
        ]
    },
    "admin": {
        "address": {
            "socket_address": {
                "address": "0.0.0.0",
                "port_value": 34901
            }
        }
    },
    "pprof": {
        "debug": true,
        "port_value": 34902
    }
}
